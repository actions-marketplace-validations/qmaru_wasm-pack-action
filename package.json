{
  "name": "wasm-pack-action",
  "version": "0.5.0",
  "private": true,
  "description": "wasm-pack action to install wasm-pack by downloading the executable",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "pack": "ncc build",
    "test": "jest",
    "all": "npm run build && npm run format && npm run pack && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jetli/wasm-pack-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup",
    "wasm-pack"
  ],
  "author": "Jet Li",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/io": "^1.1.3",
    "@actions/tool-cache": "^2.0.1",
    "typed-rest-client": "^1.8.11"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.26",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@zeit/ncc": "^0.20.5",
    "eslint": "^8.57.0",
    "eslint-plugin-github": "^4.10.2",
    "eslint-plugin-jest": "^27.9.0",
    "jest": "^29.7.0",
    "jest-circus": "^29.7.0",
    "js-yaml": "^4.1.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.2"
  }
}
